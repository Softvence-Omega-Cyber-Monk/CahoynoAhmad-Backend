model Payment {
  id        String     @id @default(uuid())
  amount    Float
  status    String
  createdAt DateTime   @default(now())
  updatedAt DateTime   @updatedAt
  planId String?
  planName String?
  userId    String?
  userEmail String?
  user      Credential? @relation(fields: [userId], references: [id], onDelete: Cascade)
}
